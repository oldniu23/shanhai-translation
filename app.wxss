/**app.wxss**/
/* icontfont需要在这里引入才能在其他地方直接用class="iconfont icon-xxx" */
@import "./assets/iconfont/iconfont.wxss";

.container {
  padding: 0;
  background-color:#f5fafe;
  height: 100vh;
  display: flex;
  flex-direction:column;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #333;
} 

.copyright {
  align-self:center;
  flex:1;
  display:flex;
  align-items:flex-end;
  padding-bottom: 30rpx;
  font-size: 28rpx;
  color:#999;
}

.view-hover {
  background-color: #f3f3f3!important;
}


